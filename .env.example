# .env.example - Copy to .env for local development (optional)
# Note: Users will provide their own keys in the UI

# OpenAI Configuration
OPENAI_API_KEY=your_openai_key_here
OPENAI_MODEL=gpt-4o-mini
OPENAI_EMBED_MODEL=text-embedding-3-small

# Groq Configuration
GROQ_API_KEY=your_groq_key_here
GROQ_MODEL=llama3-8b-8192

# Gemini Configuration
GEMINI_API_KEY=your_gemini_key_here
GEMINI_MODEL=gemini-2.5-pro

# HuggingFace Configuration
HUGGINGFACE_API_KEY=your_hf_key_here
HUGGINGFACE_EMBED_MODEL=sentence-transformers/all-mpnet-base-v2

# Processing Configuration
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
FAISS_INDEX_PATH=./vectorstore/faiss.index
FAISS_META_PATH=./vectorstore/faiss_meta.json